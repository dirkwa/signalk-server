{
  "extends": "../../../packages/assemblyscript-plugin-sdk/asconfig.json",
  "targets": {
    "release": {
      "outFile": "plugin.wasm",
      "sourceMap": false,
      "optimize": true,
      "shrinkLevel": 2,
      "converge": true,
      "noAssert": true,
      "runtime": "stub"
    },
    "debug": {
      "outFile": "build/plugin.debug.wasm",
      "sourceMap": true,
      "debug": true,
      "runtime": "stub"
    }
  },
  "options": {
    "bindings": "esm",
    "exportRuntime": false
  }
}
